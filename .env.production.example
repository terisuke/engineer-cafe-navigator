# Production Environment Variables Example
# Copy this to .env.production and fill with actual values

# Database (Supabase)
DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-REF].supabase.co:5432/postgres
POSTGRES_URL=postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-REF].supabase.co:5432/postgres
NEXT_PUBLIC_SUPABASE_URL=https://[YOUR-PROJECT-REF].supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Google Cloud
GOOGLE_CLOUD_PROJECT_ID=aipartner-426616
GOOGLE_GENERATIVE_AI_API_KEY=AIzaSyD2mRqSsY-DMurxjGu8AWt7EZgmy_SzKEs
# Path to service account key (relative to project root)
GOOGLE_APPLICATION_CREDENTIALS=config/service-account-key.json

# OpenAI (Required for V2 embeddings)
OPENAI_API_KEY=sk-proj-...

# Feature Flags (Conservative start)
FF_ENABLE_GRADUAL_MIGRATION=true
FF_NEW_EMBEDDINGS_PERCENTAGE=0
FF_USE_PARALLEL_RAG=false
FF_ENABLE_PERFORMANCE_LOGGING=true
FF_ENABLE_QUERY_ANALYTICS=true

# External APIs
ENGINEER_CAFE_CALENDAR_ID=your-calendar-id@group.calendar.google.com
CONNPASS_API_ENABLED=true
ENGINEER_CAFE_WEBSITE_URL=https://engineer-cafe.jp
SCRAPING_USER_AGENT=EngineerCafeNavigator/1.0

# Cache (Optional - Redis)
UPSTASH_REDIS_URL=https://...
UPSTASH_REDIS_TOKEN=...

# Monitoring & Alerts
ALERT_WEBHOOK_SECRET=<generate-with: openssl rand -hex 32>
CRON_SECRET=<generate-with: openssl rand -hex 32>
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/...

# Deployment
NODE_ENV=production
VERCEL_PROJECT_ID=prj_...
VERCEL_ORG_ID=team_...

# Application Settings
NEXTAUTH_URL=https://your-production-domain.com
NEXTAUTH_SECRET=<generate-with: openssl rand -hex 32>

# Optional: External System Integration
WEBSOCKET_URL=
RECEPTION_API_URL=